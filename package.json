{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API Desenvolvida em: TS, NODE, ES6, Postgres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "./node_modules/.bin/cross-env SERVER_PORT=5000 NODE_ENV=development ts-node ./bootstrap.ts",
    "builded": "./node_modules/.bin/cross-env PORT=5000 NODE_ENV=production node ./dist/server/server.js",
    "unit-test": "./node_modules/.bin/cross-env NODE_ENV=test mocha --opts dist/tests/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "./node_modules/.bin/cross-env NODE_ENV=test mocha --opts dist/tests/integration/config/mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Paulo Ferreira",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "custom-env": "^1.0.0",
    "express": "^4.16.4",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.9.0",
    "sequelize": "^5.1.1",
    "sequelize-cli": "^5.4.0",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.26",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.44",
    "@types/supertest": "^2.0.7",
    "@types/typescript": "^2.0.0",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "supertest": "^4.0.2",
    "testdouble": "^3.11.0",
    "ts-node": "^8.0.3"
  }
}
